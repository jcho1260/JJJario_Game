<?xml version = "1.0" ?>
<ScrollPane id="GLScrollPane" type="Control" style="view_resources/launcher/css/GameLibraryScroll.css">
  <VBox id="GameLibraryVBox" type="Pane" style="view_resources/launcher/css/GameLibrary.css">
    <Height>1080</Height>
    <Width>1100</Width>
    <HBox id="GameLibraryHBox1" type="Pane" style="view_resources/launcher/css/GameLibraryRow.css">
      <Button id="Game1Button" type="Leaf">
        <Text>JJJario</Text>
        <ImageView>
          <Image>
            <Path>view_resources/images/logo/JJJarioLogo.png</Path>
          </Image>
          <Ratio>true</Ratio>
          <Height>130</Height>
          <Width>150</Width>
        </ImageView>
        <Event method="setOnAction">
          <Game>JJJario</Game>
          <Type>LaunchGame</Type>
          <Path>resources/view_resources/game/JJJario/Menu.XML</Path>
        </Event>
      </Button>
      <Button id="Game2Button" type="Leaf">
        <Text>Flappy Bird</Text>
        <ImageView>
          <Image>
            <Path>view_resources/images/logo/FlappyBirdLogo.png</Path>
          </Image>
          <Ratio>true</Ratio>
          <Height>130</Height>
          <Width>150</Width>
        </ImageView>
        <Event method="setOnAction">
          <Game>FlappyBird</Game>
          <Type>LaunchGame</Type>
          <Path>resources/view_resources/game/FlappyBird/Menu.XML</Path>
        </Event>
      </Button>
      <Button id="Game3Button" type="Leaf">
        <Text>Game 3</Text>
        <!--<Action>TODO</Action>-->
      </Button>
      <Button id="Game4Button" type="Leaf">
        <Text>Game 4</Text>
        <!--<Action>TODO</Action>-->
      </Button>
    </HBox>
  </VBox>
</ScrollPane>