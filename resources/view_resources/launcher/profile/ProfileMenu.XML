<?xml version = "1.0" ?>
<VBox id="ProfileMenuVBox" type="Pane" style="view_resources/launcher/profile/css/ProfileLoginVBox.css">
  <Height>750</Height>
  <Width>1050</Width>
  <HBox id="ProfileMenuHBox1" type="Pane" style="view_resources/launcher/profile/css/ProfileMenuHBox.css">
    <ImageView id="ProfileImage" type="Leaf">
      <Image>
        <Path>view_resources/images/button_icons/User.png</Path>
      </Image>
      <Ratio>true</Ratio>
      <Height>225</Height>
      <Width>225</Width>
    </ImageView>
    <VBox id="ProfileMenuHBox2" type="Pane" style="view_resources/launcher/profile/css/ProfileMenuHBox.css">
      <TextField id="UsernameMenuInput" type="Leaf">
        <Prompt>Username</Prompt>
      </TextField>
      <TextField id="WKeyMenuInput" type="Leaf">
        <Prompt>WKey</Prompt>
      </TextField>
    </VBox>
  </HBox>
</VBox>