  <Width>320</Width>
  <ImageView id="JJJarioLogo" type="Leaf">
    <Image>
      <Path>view_resources/images/logo/JJJarioLogo.png</Path>
    </Image>
    <Width>300</Width>
    <Height>260</Height>
  </ImageView>
  <Button id="LoadLibraryButton" type="Leaf">
