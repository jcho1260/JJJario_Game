<?xml version = "1.0" ?>
<HBox style="view_resources/game/css/GameOver.css" id="GameOverH" type="Pane">
  <VBox style="view_resources/game/css/GameOver.css" id="GameOverV" type="Pane">
    <ImageView id="GOImage" type="Leaf">
      <Height>200</Height>
      <Image>
        <Path>view_resources/images/GameOver.png</Path>
      </Image>
      <Ratio>true</Ratio>
      <Width>200</Width>
    </ImageView>
    <Button id="GORLButton" type="Leaf">
      <Text>Restart Level?</Text>
      <Event method="setOnAction">
        <Type>restartLevel</Type>
      </Event>
    </Button>
    <Button id="GOMButton" type="Leaf">
      <Text>Game Menu</Text>
      <Event method="setOnAction">
        <Type>goToMenu</Type>
      </Event>
    </Button>
    <Height>420</Height>
    <Width>960</Width>
  </VBox>
</HBox>